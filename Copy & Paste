Sub ANIMAX2()
    Sheets("##").Select
    Range("A2:L13").Select
    Selection.Copy
    Sheets("TITLE LIST").Select
    Cells(Rows.Count, "A").End(xlUp).Offset(1, 0).EntireRow.Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
End Sub
